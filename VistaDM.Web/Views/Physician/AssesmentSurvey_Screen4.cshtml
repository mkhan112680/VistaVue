@Html.Hidden("IsReadOnlyMode", Model.ReadOnly)

@section scripts
{
    <script type="text/javascript">

        $(document).ready(function () {

            if ($("#IsReadOnlyMode").val() == "True") {

                //$('*', 'form').attr('disabled', 'disabled').attr('readonly', 'readonly');
                $('input[type = radio], input[type = checkbox]').attr('disabled', 'disabled').attr('readonly', 'readonly');
            }


            var UnCheckAll = function (uncheck) {

                if (uncheck) {

                    $('#squaredThree405').prop('checked', false);
                    $('#squaredThree404').prop('checked', false);
                    $('#squaredThree403').prop('checked', false);
                    $('#squaredThree402').prop('checked', false);
                    $('#squaredThree401').prop('checked', false);
                }
                else {

                    $('#squaredThree400').prop('checked', false);

                }
            }

            $('#squaredThree400').change(function () {

                if ($(this).is(':checked')) {

                    UnCheckAll(true);

                }

            });

            $('#squaredThree405').change(function () {

                if ($(this).is(':checked')) {

                    UnCheckAll(false);

                }

            });

            $('#squaredThree404').change(function () {

                if ($(this).is(':checked')) {

                    UnCheckAll(false);

                }

            });

            $('#squaredThree403').change(function () {

                if ($(this).is(':checked')) {

                    UnCheckAll(false);

                }

            });

            $('#squaredThree402').change(function () {

                if ($(this).is(':checked')) {

                    UnCheckAll(false);

                }

            });

            $('#squaredThree401').change(function () {

                if ($(this).is(':checked')) {

                    UnCheckAll(false);

                }

            });

        });
    </script>
}


